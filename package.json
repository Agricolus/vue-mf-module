{
  "name": "vue-mf-module",
  "version": "1.0.0",
  "description": "A vue extension to create microfrontend module",
  "module": "dist/vue-mf-module.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "dist/vue-mf-module.mjs",
  "exports": {
    ".": {
      "import": "./dist/vue-mf-module.mjs",
      "require": "./dist/vue-mf-module.umd.js"
    }
  },
  "scripts": {
    "compile": "tsc",
    "build": "vue-tsc --declaration --emitDeclarationOnly && vite build"
  },
  "author": "e.sodacci <e.sodacci@agricolus.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Agricolus/vue-mf-module/issues"
  },
  "homepage": "https://github.com/Agricolus/vue-mf-module",
  "keywords": [
    "vue",
    "mf"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agricolus/vue-mf-module.git"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue2": "^2.0.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "@rollup/plugin-typescript": "^10.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vue-tsc": "^1.0.9"
  },
  "peerDependencies": {
    "tiny-emitter": "2.1.0",
    "typescript": "^4.8.4",
    "vue": "^2.7.14"
  }
}
